<Window x:Class="RequestMaker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RequestMaker"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnReq" Content="Make Request" HorizontalAlignment="Left" Margin="31,80,0,0" VerticalAlignment="Top" Width="106" Click="Button_Click"/>
        <TextBox x:Name="searhBox" HorizontalAlignment="Left" Height="23" Margin="30,52,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="244"/>
        <ComboBox x:Name="cBoxType" HorizontalAlignment="Left" Margin="30,10,0,0" VerticalAlignment="Top" Width="244"/>
        <TextBox x:Name="logBox" HorizontalAlignment="Left" Height="193" Margin="31,186,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="243"/>
        <Button x:Name="btnResp" Content="Get Respond" HorizontalAlignment="Left" Margin="167,80,0,0" VerticalAlignment="Top" Width="107" Click="Button_Click"/>

    </Grid>
</Window>
